<section>

	{{#each pool}}
		<a href="detail.html?id={{ID}}"><img src="{{thumb}}">
		
			<div>
				{{#if designer_brand.profile_pic}}
					<img src="{{designer_brand.profile_pic}}" class="profile_pic round" />
				{{else}}
					<i class="material-icons account_circle invert" ></i>
				{{/if}}
				<h1>{{product_title}}</h1>
				<span>{{designer_brand.name}}</span>
				<h2>$ {{price}}</h2>
			</div>
		</a>
	{{/each}}

</section>